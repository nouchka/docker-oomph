          key="/instance/org.eclipse.php.debug.core/org.eclipse.php.debug.coreinstalledPHPDebuggers"
          value="org.eclipse.php.debug.core.noneDebugger"/>
      <setupTask
          xsi:type="setup:PreferenceTask"
